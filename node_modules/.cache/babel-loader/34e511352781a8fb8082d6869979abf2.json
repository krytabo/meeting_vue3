{"ast":null,"code":"import NavbarHeader from \"@/components/Navbar/header\";\nimport sideMenu from \"@/components/Navbar/sideMenu\";\nimport zhTW from \"@arco-design/web-vue/es/locale/lang/zh-tw\";\nexport default {\n  name: \"app\",\n  components: {\n    NavbarHeader,\n    sideMenu\n  },\n  provide() {\n    return {\n      reload: this.reload\n    };\n  },\n  data() {\n    return {\n      zhTW,\n      isReload: true,\n      collapsed: true,\n      checkHidden: false,\n      selected: 0,\n      tabsList: [{\n        id: 0,\n        name: \"定價表\",\n        link: \"/\"\n      }, {\n        id: 1,\n        name: \"配置圖設定\",\n        link: \"/config-setting\"\n      }],\n      active: false,\n      headerTitle: \"會議記錄系統\"\n    };\n  },\n  methods: {\n    listClose() {\n      this.checkHidden = !this.checkHidden;\n    },\n    reload() {\n      this.isReload = false;\n      this.$nextTick(() => {\n        this.isReload = true;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,YAAW,MAAO,4BAA4B;AACrD,OAAOC,QAAO,MAAO,8BAA8B;AACnD,OAAOC,IAAG,MAAO,2CAA2C;AAE5D,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,YAAY;IACZC;EACF,CAAC;EACDI,OAAO,GAAG;IACR,OAAO;MACLC,MAAM,EAAE,IAAI,CAACA;IACf,CAAC;EACH,CAAC;EACDC,IAAI,GAAG;IACL,OAAO;MACLL,IAAI;MACJM,QAAQ,EAAE,IAAI;MACdC,SAAS,EAAE,IAAI;MACfC,WAAW,EAAE,KAAK;MAClBC,QAAQ,EAAE,CAAC;MACXC,QAAQ,EAAE,CACR;QACEC,EAAE,EAAE,CAAC;QACLV,IAAI,EAAE,KAAK;QACXW,IAAI,EAAE;MACR,CAAC,EACD;QACED,EAAE,EAAE,CAAC;QACLV,IAAI,EAAE,OAAO;QACbW,IAAI,EAAE;MACR,CAAC,CACF;MACDC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,IAAI,CAACR,WAAU,GAAI,CAAC,IAAI,CAACA,WAAW;IACtC,CAAC;IACDJ,MAAM,GAAG;MACP,IAAI,CAACE,QAAO,GAAI,KAAK;MACrB,IAAI,CAACW,SAAS,CAAC,MAAM;QACnB,IAAI,CAACX,QAAO,GAAI,IAAI;MACtB,CAAC,CAAC;IACJ;EACF;AACF,CAAC","names":["NavbarHeader","sideMenu","zhTW","name","components","provide","reload","data","isReload","collapsed","checkHidden","selected","tabsList","id","link","active","headerTitle","methods","listClose","$nextTick"],"sourceRoot":"","sources":["/Users/krystal/Vue/meeting_vue3/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <n-config-provider :locale=\"zhTW\" :date-locale=\"dateZhTW\">\n      <n-layout class=\"h-screen\">\n        <n-layout-header bordered>\n          <navbar-header :title=\"headerTitle\"></navbar-header>\n        </n-layout-header>\n        <n-layout position=\"absolute\" style=\"top: 60px\" has-sider>\n          <n-layout-sider\n            collapse-mode=\"width\"\n            :collapsed-width=\"0\"\n            :width=\"240\"\n            show-trigger=\"arrow-circle\"\n            content-style=\"padding: 24px;background-color: #232324;height:100%\"\n            bordered\n            :native-scrollbar=\"false\"\n          >\n            <side-menu class=\"body-height\" :collapse=\"collapsed\"></side-menu>\n          </n-layout-sider>\n          <n-layout content-style=\"padding: 20px\" :native-scrollbar=\"false\">\n            <a-config-provider :locale=\"zhTW\">\n              <router-view class=\"overflow-auto\" v-if=\"isReload\" />\n            </a-config-provider>\n          </n-layout>\n        </n-layout>\n      </n-layout>\n    </n-config-provider>\n    <!--<a-config-provider :locale=\"zhTW\">\n      <div class=\"body-height absolute top-16 flex w-full overflow-hidden\">\n        &lt;!&ndash;<div class=\"sideMenu_model\" :class=\"{ sideMenu_hidden: checkHidden === true }\">\n        <div v-for=\"(item, index) in tabsList\" :key=\"index\" class=\"menu flex flex-col items-center space-y-1\">\n          <router-link :to=\"item.link\" class=\"w-full rounded px-4 py-3 text-left text-white hover:bg-white hover:bg-opacity-10\">{{ item.name }}</router-link>\n        </div>\n        <div @click=\"listClose\" class=\"listHidden-button\" :class=\"{ listShow_button: checkHidden === true }\">\n          <i v-if=\"checkHidden === false\" class=\"ri-arrow-left-s-line\"></i>\n          <i v-else class=\"ri-arrow-right-s-line text-white\"></i>\n        </div>\n      </div>&ndash;&gt;\n        <side-menu class=\"body-height\"></side-menu>\n        <router-view v-if=\"isReload\" />\n      </div>\n    </a-config-provider>-->\n  </div>\n</template>\n\n<script>\nimport NavbarHeader from \"@/components/Navbar/header\";\nimport sideMenu from \"@/components/Navbar/sideMenu\";\nimport zhTW from \"@arco-design/web-vue/es/locale/lang/zh-tw\";\n\nexport default {\n  name: \"app\",\n  components: {\n    NavbarHeader,\n    sideMenu,\n  },\n  provide() {\n    return {\n      reload: this.reload,\n    };\n  },\n  data() {\n    return {\n      zhTW,\n      isReload: true,\n      collapsed: true,\n      checkHidden: false,\n      selected: 0,\n      tabsList: [\n        {\n          id: 0,\n          name: \"定價表\",\n          link: \"/\",\n        },\n        {\n          id: 1,\n          name: \"配置圖設定\",\n          link: \"/config-setting\",\n        },\n      ],\n      active: false,\n      headerTitle: \"會議記錄系統\",\n    };\n  },\n  methods: {\n    listClose() {\n      this.checkHidden = !this.checkHidden;\n    },\n    reload() {\n      this.isReload = false;\n      this.$nextTick(() => {\n        this.isReload = true;\n      });\n    },\n  },\n};\n</script>\n<style lang=\"scss\">\nbody {\n  background: #f1f1f1;\n}\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n}\n\n@media only screen and (max-width: 500px) {\n  .body-height {\n    height: calc(100vh - 150px) !important;\n    padding-bottom: env(safe-area-bottom);\n  }\n}\n.body-height {\n  height: calc(100vh - 64px);\n}\n\n.body-main {\n  display: block;\n  flex: 1;\n  flex-basis: auto;\n  overflow: auto;\n  box-sizing: border-box;\n}\n\n.sideMenu_model {\n  @apply absolute z-50 flex w-full flex-col space-y-5 bg-gray-900 p-4 pr-20 md:relative md:w-80;\n\n  .router-link-active {\n    @apply border border-blue-500 bg-blue-600 bg-opacity-20;\n  }\n}\n\n.listHidden-button {\n  @apply absolute right-5 top-2 flex h-8 w-8 cursor-pointer items-center justify-center rounded-full bg-white shadow-6;\n}\n\n.listShow_button {\n  @apply -right-14 top-2 bg-gray-800;\n}\n\n.sideMenu_hidden {\n  @apply absolute -left-8 mr-0 w-0 pr-0 md:-left-8;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}